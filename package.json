{
  "name": "@artevelde-uas/canvas-lms-speedgrader-letter-grade-dropdown-plugin",
  "version": "0.5.1",
  "description": "Adds a convenient dropdown list to the speedgrader when using letter grades",
  "keywords": [
    "Canvas LMS"
  ],
  "license": "ISC",
  "author": "Renaat De Muynck <renaat.demuynck@arteveldehs.be>",
  "repository": {
    "type": "git",
    "url": "https://github.com/artevelde-uas/canvas-lms-speedgrader-letter-grade-dropdown-plugin.git"
  },
  "module": "src/index.js",
  "files": [
    "src/",
    "locales/"
  ],
  "packageManager": "yarn@4.9.1",
  "scripts": {
    "dev": "run-p serve watch",
    "watch": "webpack ./index.js --watch --progress --stats minimal --config webpack.config.js",
    "build": "canvas-build",
    "serve": "canvas-server-start"
  },
  "peerDependencies": {
    "@artevelde-uas/canvas-lms-app": "^1.14.0"
  },
  "devDependencies": {
    "@artevelde-uas/canvas-lms-app": "1",
    "@artevelde-uas/canvas-lms-theme-dev": "0.4",
    "npm-run-all": "^4.1.5"
  },
  "config": {
    "scope": {
      "course": true
    },
    "roles": {
      "teacher": true
    }
  }
}
